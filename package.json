{
  "name": "@vernonia/core",
  "version": "4.1.0",
  "description": "City of Vernonia friends for ArcGIS Maps SDK for JavaScript.",
  "type": "module",
  "scripts": {
    "assets": "node ./scripts/assets.js",
    "build": "rimraf dist && tsc && sass src:dist && node ./scripts/minify.js",
    "dev": "vite --config ./dev/vite.config.js",
    "docs": "typedoc",
    "lint": "eslint",
    "prettier": "prettier --write \"src/**/*.ts?(x)\"",
    "test": "npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfVernonia/core.git"
  },
  "author": "COV-GIS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CityOfVernonia/core/issues"
  },
  "homepage": "https://github.com/CityOfVernonia/core#readme",
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@mxssfd/typedoc-theme": "^1.1.7",
    "@types/js-cookie": "^3.0.6",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.14.11",
    "@types/papaparse": "^5.3.14",
    "@types/pubsub-js": "^1.8.6",
    "@types/sortablejs": "^1.15.8",
    "@types/uuid": "^10.0.0",
    "chalk": "^5.3.0",
    "eslint": "^9.10.0",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "replace-in-file": "^8.1.0",
    "rimraf": "^6.0.1",
    "sass": "^1.79.1",
    "terser": "^5.33.0",
    "tslib": "^2.7.0",
    "typedoc": "^0.26.7",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.6.0",
    "vite": "^5.4.6"
  },
  "dependencies": {
    "@arcgis/core": "^4.30.9",
    "@esri/calcite-components": "^2.12.2",
    "fast-xml-parser": "^4.5.0",
    "interactjs": "^1.10.27",
    "js-cookie": "^3.0.5",
    "luxon": "^3.5.0",
    "papaparse": "^5.4.1",
    "pubsub-js": "^1.9.4"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120,
    "tabWidth": 2,
    "endOfLine": "auto"
  }
}
