{
  "name": "@vernonia/core",
  "version": "5.1.15",
  "description": "City of Vernonia friends for ArcGIS Maps SDK for JavaScript.",
  "type": "module",
  "scripts": {
    "assets": "node ./scripts/assets.js",
    "build": "rimraf dist && tsc && sass src:dist && node ./scripts/minify.js",
    "dev": "vite --config ./dev/vite.config.js",
    "docs": "rimraf docs && typedoc",
    "lint": "eslint",
    "prettier": "prettier --write \"src/**/*.ts?(x)\"",
    "prerelease": "npm install && npm test && npm run build && npm run docs",
    "test": "npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfVernonia/core.git"
  },
  "author": "COV-GIS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CityOfVernonia/core/issues"
  },
  "homepage": "https://github.com/CityOfVernonia/core#readme",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/js-cookie": "^3.0.6",
    "@types/luxon": "^3.7.1",
    "@types/papaparse": "^5.3.16",
    "@types/pubsub-js": "^1.8.6",
    "chalk": "^5.6.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "fs-extra": "^11.3.2",
    "glob": "^11.0.3",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "sass": "^1.93.1",
    "terser": "^5.44.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.13",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "@arcgis/core": "4.33.14",
    "@arcgis/map-components": "^4.33.21",
    "@esri/calcite-components": "^3.3.0",
    "fast-xml-parser": "^5.2.5",
    "js-cookie": "^3.0.5",
    "luxon": "^3.7.2",
    "marked": "^16.3.0",
    "papaparse": "^5.5.3",
    "pdfjs-dist": "^5.4.149",
    "pubsub-js": "^1.9.5"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120,
    "tabWidth": 2,
    "endOfLine": "auto"
  }
}
