{
  "name": "@vernonia/core",
  "version": "5.0.17",
  "description": "City of Vernonia friends for ArcGIS Maps SDK for JavaScript.",
  "type": "module",
  "scripts": {
    "assets": "node ./scripts/assets.js",
    "build": "rimraf dist && tsc && sass src:dist && node ./scripts/minify.js",
    "dev": "vite --config ./dev/vite.config.js",
    "docs": "rimraf docs && typedoc",
    "lint": "eslint",
    "prettier": "prettier --write \"src/**/*.ts?(x)\"",
    "prerelease": "npm install && npm test && npm run build && npm run docs",
    "test": "npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfVernonia/core.git"
  },
  "author": "COV-GIS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CityOfVernonia/core/issues"
  },
  "homepage": "https://github.com/CityOfVernonia/core#readme",
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/js-cookie": "^3.0.6",
    "@types/luxon": "^3.6.2",
    "@types/papaparse": "^5.3.15",
    "@types/pubsub-js": "^1.8.6",
    "chalk": "^5.4.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "replace-in-file": "^8.3.0",
    "rimraf": "^6.0.1",
    "sass": "^1.86.3",
    "terser": "^5.39.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.6"
  },
  "dependencies": {
    "@arcgis/core": "4.32.10",
    "@arcgis/map-components": "^4.32.14",
    "@esri/calcite-components": "^3.1.0",
    "fast-xml-parser": "^5.2.0",
    "js-cookie": "^3.0.5",
    "luxon": "^3.6.1",
    "marked": "^15.0.8",
    "papaparse": "^5.5.2",
    "pubsub-js": "^1.9.5"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120,
    "tabWidth": 2,
    "endOfLine": "auto"
  }
}
